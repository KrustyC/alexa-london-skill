{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "import/imports-first": ["error", "absolute-first"],
    "import/newline-after-import": "error"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  }
}
